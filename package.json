{
  "name": "old-school-img-loader",
  "description": "a fun component to slowly load images like they did in old (yet futuristic) movies and tv shows",
  "author": "nicerhugs <jscheuring@gmail.com> (http://nicerhugs.com)",
  "version": "0.1.0",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "babel src --out-dir lib && npm run compile:examplejs",
    "compile:examplejs": "browserify example/src/main.js -o example/bundle.js"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NicerHugs/old-school-img-loader.git"
  },
	"license": "WTFPL",
  "bugs": "https://github.com/NicerHugs/old-school-img-loader/issues",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0"
  },
  "dependencies": {
    "react": "^15.3.1",
    "react-dom": "^15.3.1"
  }
}
